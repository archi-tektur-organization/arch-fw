{
  "name": "archi-tektur/arch-fw",
  "type": "framework",
  "description": "Boilerplate for ArchFW",
  "homepage": "https://archi-tektur.pl/archFW",
  "license": "MIT",
  "version": "3.0.0-stable",
  "authors": [
    {
      "name": "Oskar 'archi_tektur' Barcz",
      "email": "kontakt@archi-tektur.pl",
      "homepage": "https://www.archi-tektur.pl",
      "role": "Developer"
    }
  ],
  "config": {
    "medoo-database-data": "config/database.php",
    "framework-settings": "config/archsettings.php",
    "router": "config/routes.php"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "ArchFW\\": "fw-src/"
    }
  },
  "require": {
    "php": "7.3.*",
    "ext-json": "*",
    "ext-pdo": "*",
    "twig/twig": "2.9.*",
    "catfan/medoo": "v1.6.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  }
}
